package com.flf.entity;

import java.math.BigDecimal;

import com.base.entity.BaseEntity;

/**
 * 
 * <br>
 * <b>功能：</b>ShopTradingHistoryEntity<br>
 */
public class ShopTradingHistory extends BaseEntity {

	private java.lang.Integer id;//
	private java.lang.Integer userId;// 用户表id（外键）
	private java.lang.String tradingContent;// 交易内容
	private java.lang.String createTime;// 交易时间
	private java.lang.Integer tradingStatus;// 交易状态1成功0失败
	private BigDecimal money;// 交易金额
	private BigDecimal balance;// 余额
	private java.lang.Integer type;// 减少或增加(1增减0减少)
	private java.lang.String quickComment;//
	private java.lang.String remark3;//

	private Page page;

	public java.lang.Integer getId() {
		return this.id;
	}

	public void setId(java.lang.Integer id) {
		this.id = id;
	}

	public java.lang.Integer getUserId() {
		return this.userId;
	}

	public void setUserId(java.lang.Integer userId) {
		this.userId = userId;
	}

	public java.lang.String getTradingContent() {
		return this.tradingContent;
	}

	public void setTradingContent(java.lang.String tradingContent) {
		this.tradingContent = tradingContent;
	}

	public java.lang.String getCreateTime() {
		return this.createTime;
	}

	public void setCreateTime(java.lang.String createTime) {
		this.createTime = createTime;
	}

	public java.lang.Integer getTradingStatus() {
		return this.tradingStatus;
	}

	public void setTradingStatus(java.lang.Integer tradingStatus) {
		this.tradingStatus = tradingStatus;
	}

	public BigDecimal getMoney() {
		return this.money;
	}

	public void setMoney(BigDecimal money) {
		this.money = money;
	}

	public BigDecimal getBalance() {
		return this.balance;
	}

	public void setBalance(BigDecimal balance) {
		this.balance = balance;
	}

	public java.lang.Integer getType() {
		return this.type;
	}

	public void setType(java.lang.Integer type) {
		this.type = type;
	}

	public java.lang.String getQuickComment() {
		return this.quickComment;
	}

	public void setQuickComment(java.lang.String quickComment) {
		this.quickComment = quickComment;
	}

	public java.lang.String getRemark3() {
		return this.remark3;
	}

	public void setRemark3(java.lang.String remark3) {
		this.remark3 = remark3;
	}

	public Page getPage() {
		return page;
	}

	public void setPage(Page page) {
		this.page = page;
	}


}
